            department:
              code: code
              name: name
        temperature:
          indicator: indicator
          heatSensitive: true
        description: description
        upc: upc
        categories:
          - categories
          - categories
        countryOrigin: countryOrigin
        brand: brand
        items:
          - ''
          - ''
      meta: '{}'
  ProductsPayloadModel:
    type: object
    properties:
      data:
