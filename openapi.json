swagger: '2.0'
info:
  description: |2
     A set of APIs have been made available for applications to integrate with The Kroger Co. family of stores data.
  version: 1.0.0
  title: The Kroger Co.
  termsOfService: 'https://www.kroger.com/topic/terms-and-conditions-2'
  contact:
    name: API Support
    email: APISupport@kroger.com
# basePath: /v1
schemes:
  - https
consumes:
  - application/json
produces:
  - application/json
security:
  - Public:
      - profile.full
      - profile.basic
      - profile.loyalty
      - product.personalized
      - cart.basic
      - 'cart.basic:rw'
paths:
  /products:
    get:
      tags:
        - products
      summary: Returns a list of all products
      description: product api get function
      operationId: productGet
      produces:
        - application/json
      parameters:
        - name: filter.term
          in: query
          description: 'Input term to filter by (i.e. milk, bread, salt)'
          required: false
          type: string
        - name: filter.locationId
          in: query
          description: LocationID for a Kroger Family of stores location
          required: false
          type: string
          maxLength: 8
          minLength: 8
        - name: filter.productId
          in: query
          description: >-
            A comma-separated list of product ids to retrieve. Must be used with
            location id filter to enhance items with prices. Ignores all other
            filters when used.
          required: false
          type: string
        - name: filter.brand
          in: query
          description: >-
            A pipe-separated list of case-sensitive brand names to be applied as
            filters
          required: false
          type: string
        - name: filter.fulfillment
          in: query
          description: >-
            A comma-separated list of fulfillment types; case-insensitive. 'ais'
            is Available In Store, 'csp' is Curb Side Pickup, 'dth' is Delivery
            To Home, 'sth' is Ship To Home
          required: false
          type: string
        - name: filter.start
          in: query
          description: The number of records to skip
          required: false
          type: integer
        - name: filter.limit
          in: query
          description: The number of records to return to a maximum of 200
          required: false
          type: integer
      responses:
        '200':
          description: OK
          schema:
            $ref: '#/definitions/ProductsPayloadModel'
        '400':
          description: BAD REQUEST
          schema:
            $ref: '#/definitions/APIError'
      security:
        - Public:
            - product.personalized
  '/products/{id}':
    get:
      tags:
        - products
      summary: Returns one product based on item id or upc
      description: ''
      operationId: productGetID
      produces:
        - application/json
      parameters:
        - name: id
          in: path
          required: true
          type: string
          maxLength: 13
          minLength: 13
        - name: filter.locationId
          in: query
          description: LocationID for a Kroger Family of stores location
          required: false
          type: string
          maxLength: 8
          minLength: 8
      responses:
        '200':
          description: OK
          schema:
            $ref: '#/definitions/ProductPayloadModel'
  /carts:
    get:
      tags:
        - carts
      summary: Returns a list of all carts for user
      description: cart api get function
      operationId: getCarts
      produces:
        - application/json
      parameters: []
      responses:
        '200':
          description: OK
          schema:
            $ref: '#/definitions/CartsPayloadModel'
        '400':
          description: BAD REQUEST
          schema:
            $ref: '#/definitions/APIError'
      security:
        - Public:
            - cart.basic
    post:
      tags:
        - carts
      summary: Create new cart for user
      description: ''
      operationId: createCart
      consumes:
        - application/json
      produces:
        - application/json
      parameters:
        - in: body
          name: items
          required: false
          schema:
            $ref: '#/definitions/CartRequestModel'
      responses:
        '201':
          description: Created
          schema:
            $ref: '#/definitions/CartsPayloadModel'
      security:
        - Public:
            - 'cart.basic:rw'
  '/carts/{id}':
    get:
      tags:
        - carts
      summary: Returns a cart based on the id
      description: cart api get function
      operationId: getCart
      consumes:
        - application/json
      produces:
        - application/json
      parameters:
        - name: id
          in: path
          required: true
          type: integer
      responses:
        '200':
          description: OK
          schema:
            $ref: '#/definitions/CartPayloadModel'
        '400':
          description: BAD REQUEST
          schema:
            $ref: '#/definitions/APIError'
        '409':
          description: cart not fulfillable
          schema:
            $ref: '#/definitions/APIError'
      security:
        - Public:
            - cart.basic
    put:
      tags:
        - carts
      summary: Update cart for a user
      operationId: putCart
      consumes:
        - application/json
      produces:
        - application/json
      parameters:
        - name: id
          in: path
          required: true
          type: integer
        - in: body
          name: items
          required: false
          schema:
            $ref: '#/definitions/CartRequestModel'
      responses:
        '200':
          description: OK
          schema:
            $ref: '#/definitions/CartsPayloadModel'
        '400':
          description: BAD REQUEST
          schema:
            $ref: '#/definitions/APIError'
        '409':
          description: cart not fulfillable
          schema:
            $ref: '#/definitions/APIError'
      security:
        - Public:
            - 'cart.basic:rw'
  '/carts/{id}/items':
    post:
      tags:
        - carts
      summary: Adds item in a cart for a user
      operationId: postCartItem
      consumes:
        - application/json
      produces:
        - application/json
      parameters:
        - name: id
          in: path
          required: true
          type: string
        - in: body
          name: items
          required: false
          schema:
            $ref: '#/definitions/CartItemPostRequestModel'
      responses:
        '201':
          description: created
        '400':
          description: BAD REQUEST
          schema:
            $ref: '#/definitions/APIError'
        '409':
          description: cart not fulfillable
          schema:
            $ref: '#/definitions/APIError'
      security:
        - Public:
            - 'cart.basic:rw'
  '/carts/{id}/items/{upc}':
    put:
      tags:
        - carts
      summary: Updates quantity for an item in a cart for a user
      operationId: putCartItem
      consumes:
        - application/json
      produces:
        - application/json
      parameters:
        - name: id
          in: path
          required: true
          type: string
        - name: upc
          in: path
          required: true
          type: string
          maxLength: 13
          minLength: 13
        - in: body
          name: items
          required: false
          schema:
            $ref: '#/definitions/CartItemPutRequestModel'
      responses:
        '204':
          description: NO CONTENT
        '400':
          description: BAD REQUEST
          schema:
            $ref: '#/definitions/APIError'
        '409':
          description: cart not fulfillable
          schema:
            $ref: '#/definitions/APIError'
      security:
        - Public:
            - 'cart.basic:rw'
    delete:
      tags:
        - carts
      summary: Deletes item from a cart for a user
      operationId: deleteCartItem
      produces:
        - application/json
      parameters:
        - name: id
          in: path
          required: true
          type: string
        - name: upc
          in: path
          required: true
          type: string
          maxLength: 13
          minLength: 13
      responses:
        '204':
          description: NO CONTENT
        '400':
          description: BAD REQUEST
          schema:
            $ref: '#/definitions/APIError'
        '409':
          description: cart not fulfillable
          schema:
            $ref: '#/definitions/APIError'
      security:
        - Public:
            - profile.full
  /chains:
    get:
      tags:
        - locations
      summary: Get a list of chains
      description: Get a list of chains
      operationId: ListChains
      consumes:
        - application/json
      produces:
        - application/json
      parameters: []
      responses:
        '200':
          description: OK
          schema:
            $ref: '#/definitions/locations.ChainsResponse'
        '400':
          description: BAD REQUEST
          schema:
            $ref: '#/definitions/APIError'
      security: []
  '/chains/{name}':
    get:
      tags:
        - locations
      summary: Get chain details for a specific chain name
      description: Get chain details for a specific chain name
      operationId: GetChain
      consumes:
        - application/json
      produces:
        - application/json
      parameters:
        - name: name
          in: path
          description: Chain Name
          required: true
          type: string
      responses:
        '200':
          description: OK
          schema:
            $ref: '#/definitions/locations.ChainResponse'
        '404':
          description: NOT FOUND
      security: []
    head:
      tags:
        - locations
      summary: Determine if a specific chain exists
      description: Determine if a specific chain exists
      operationId: ChainExists
      parameters:
        - name: name
          in: path
          description: Chain Name
          required: true
          type: string
      responses:
        '204':
          description: OK
        '404':
          description: NOT FOUND
      security: []
  /departments:
    get:
      tags:
        - locations
      summary: Get a list of departments
      description: Get a list of departments
      operationId: ListDepartments
      consumes:
        - application/json
      produces:
        - application/json
      parameters: []
      responses:
        '200':
          description: OK
          schema:
            $ref: '#/definitions/locations.DepartmentsResponse'
      security: []
  '/departments/{id}':
    get:
      tags:
        - locations
      summary: Get department details for a specific department id
      description: Get department details for a specific department id
      operationId: GetDepartment
      consumes:
        - application/json
      produces:
        - application/json
      parameters:
        - name: id
          in: path
          description: Department ID
          required: true
          type: string
      responses:
        '200':
          description: OK
          schema:
            $ref: '#/definitions/locations.DepartmentResponse'
        '400':
          description: BAD REQUEST
          schema:
            $ref: '#/definitions/APIError'
        '404':
          description: NOT FOUND
      security: []
    head:
      tags:
        - locations
      summary: Determine if a specific department exists
      description: Determine if a specific department exists
      operationId: DepartmentExists
      parameters:
        - name: id
          in: path
          description: Department ID
          required: true
          type: string
      responses:
        '204':
          description: OK
        '404':
          description: NOT FOUND
      security: []
  /locations:
    get:
      tags:
        - locations
      summary: Get a list of locations matching the given criteria.
      description: >-
        <p>Get a list of locations matching the given criteria. You must specify
        a starting point for the search, either as a Zip Code (using the zipCode
        query parameter), or a Latitude and Longitude (using the lat and lon or
        latLong query parameter). If multiple query parameters are present for
        the starting point, an error will be returned asking you to reduce the
        number of starting points to a single point. The default distance limit
        is 10 miles from the center point of the location coordinate. The
        default result limit is 10 locations.
      operationId: SearchLocations
      consumes:
        - application/json
      produces:
        - application/json
      parameters:
        - name: filter.zipCode.near
          in: query
          description: >-
            Zip Code (required, unless filter.latLong.near or filter.lat.near
            and filter.lon.near are provided)
          required: false
          type: string
        - name: filter.latLong.near
          in: query
          description: >-
            Latitude,Longitude (required, unless filter.zipCode.near or
            filter.lat.near and filter.lon.near are provided)
          required: false
          type: string
        - name: filter.lat.near
          in: query
          description: >-
            Latitude (required, unless filter.zipCode.near or
            filter.latLong.near are provided)
          required: false
          type: string
        - name: filter.lon.near
          in: query
          description: >-
            Longitude (required, unless filter.zipCode.near or
            filter.latLong.near are provided)
          required: false
          type: string
        - name: filter.radiusInMiles
          in: query
          description: >-
            Seach Area Limit (optional, limits the search area of the results
            displayed from the center of the search location. Range is between 1
            and 100, if not provided the default limit is 10 miles)
          required: false
          type: integer
        - name: filter.limit
          in: query
          description: >-
            Result Limit (optional, limits the number of results displayed.
            Range is between 1 and 200, if not provided the default result size
            of 10 is provided)
          required: false
          type: integer
        - name: filter.chain
          in: query
          description: >-
            Chain Name (optional, only stores matching this chain will be
            returned.)
          required: false
          type: string
        - name: filter.department
          in: query
          description: >-
            Department ID (optional, A comma-separated list of departments. Only
            stores who have all of the departments specified will be returned.)
          required: false
          type: string
      responses:
        '200':
          description: OK
          schema:
            $ref: '#/definitions/locations.LocationSearchResponse'
        '400':
          description: BAD REQUEST
          schema:
            $ref: '#/definitions/APIError'
      security: []
  '/locations/{locationId}':
    get:
      tags:
        - locations
      summary: Returns one location based on location ID.
      description: >-
        Retrieves details for a single location for a given locationID.  This is
        the same locationID that is returned from the locations search.  It is
        an 8-digit ID, composed of the Management Division Number followed by
        the Store Number.
      operationId: LocationsGetByID
      produces:
        - application/json
      parameters:
        - name: locationId
          in: path
          required: true
          type: string
          maxLength: 8
          minLength: 8
      responses:
        '200':
          description: OK
          schema:
            $ref: '#/definitions/locations.LocationResponse'
        '400':
          description: incorrect locationId format
          schema:
            $ref: '#/definitions/APIError'
        '404':
          description: location not found
        '500':
          description: error looking up location
      security: []
    head:
      tags:
        - locations
      summary: Determines existence of one location based on location ID.
      description: >-
        Determines existence of one location for a given locationID.  This is
        the same locationID that is returned from the locations search.  It is
        an 8-digit ID, composed of the Management Division Number followed by
        the Store Number.
      operationId: LocationsExistsByID
      produces:
        - application/json
      parameters:
        - name: locationId
          in: path
          required: true
          type: string
          maxLength: 8
          minLength: 8
      responses:
        '204':
          description: location exists
        '400':
          description: incorrect locationId format
          schema:
            $ref: '#/definitions/APIError'
        '404':
          description: location does not exist
        '500':
          description: error looking up location
      security: []
securityDefinitions:
  Public:
    description: >2
       The Kroger Co. utilizes the OAuth2 protocol to the RFC Specification (RFC6749). Currently we support both the **_authorization code_** and **_client credentials_** flow. Most applications/integrations will leverage the **_authorization code_** flow while service to service integrations will leverage the limited **_client credentials_** flow.

      Please refer to [OAuth RFC](https://tools.ietf.org/html/rfc6749) to find
      out in details how all those flows work and pick the right one for your
      use case.
    type: oauth2
    authorizationUrl: 'https://api.kroger.com/v1/connect/oauth2/authorize'
    tokenUrl: 'https://api.kroger.com/v1/connect/oauth2/token'
    flow: accessCode
    scopes:
      profile.full: >-
        Grants read access to profile information only. Note that this scope
        includes profile.basic and profile.loyalty.
      profile.basic: >-
        Grants read access to profile information only. Includes email, name,
        address and phone number.
      profile.loyalty: >-
        Grants read access to profile loyalty information only. Includes loyalty
        card number.
      product.personalized: Grants read access to personalized product information.
      cart.basic: Grants read access to the basic version of the cart.
      'cart.basic:rw': Grants read/write access to the basic version of the cart.
definitions:
  CartModel:
    type: object
    properties:
      id:
        type: string
      createdDate:
        type: string
        example: '2018-01-18T16:05:05.206Z'
      items:
        type: array
        items:
          $ref: '#/definitions/CartItemResponseModel'
      name:
        type: string
    example:
      createdDate: '2018-01-18T16:05:05.206Z'
      name: name
      id: id
      items:
        - createdDate: '2018-01-18T16:05:05.206Z'
          quantity: 0
          allowSubstitutes: true
          specialInstructions: specialInstructions
          upc: upc
          description: description
        - createdDate: '2018-01-18T16:05:05.206Z'
          quantity: 0
          allowSubstitutes: true
          specialInstructions: specialInstructions
          upc: upc
          description: description
  CartPayloadModel:
    type: object
    properties:
      data:
        $ref: '#/definitions/CartModel'
      meta:
        type: object
        properties: {}
    example:
      data:
        createdDate: '2018-01-18T16:05:05.206Z'
        name: name
        id: id
        items:
          - createdDate: '2018-01-18T16:05:05.206Z'
            quantity: 0
